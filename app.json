{
  "expo": {
    "name": "Travel",
    "slug": "Travel",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/app-development.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-development.png",
      "resizeMode": "contain",
      "backgroundColor": "#003b88"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDMnOxLE7mVEUTFOPH9YRrfy5waYX4OUjY"
        }
      },
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.stefanorucci.Travel"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app needs access to yout camera in order to take photoso of your favorite places."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1a9a72dd-18ff-49c6-8352-32c09a957f32"
      }
    }
  }
}
